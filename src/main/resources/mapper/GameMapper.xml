            <result property="gameDev" column="gameDev" jdbcType="VARCHAR"/>
            <result property="gamePub" column="gamePub" jdbcType="VARCHAR"/>
            <result property="gameIsRemoved" column="gameIsRemoved" jdbcType="TINYINT"/>
